{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.md"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/includes/**"
          ]
        }
      ],
      "dest": "_site/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "cwd": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "cwd": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "_appTitle": "Goal-Driven Development Kit",
      "_appName": "Goal Kit",
      "_appDescription": "Build software that achieves meaningful outcomes",
      "_appLogoPath": "../media/logo_small.webp",
      "_appFaviconPath": "../media/favicon.ico",
      "_enableSearch": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/github/goal-dev-kit",
        "branch": "main"
      },
      "_gitUrlPattern": "github"
    },
    "fileMetadata": {},
    "template": [
      "default",
      "templates/custom"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}